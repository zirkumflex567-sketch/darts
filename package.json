  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.ts?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|expo|expo-.*|@expo|react-native-gesture-handler|react-native-reanimated)/)"
    ]
  }
